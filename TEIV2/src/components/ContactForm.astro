<form class="contact-form" method="post" action="#">
  <label>
    Name
    <input type="text" name="name" required />
  </label>
  <label>
    Email
    <input type="email" name="email" required />
  </label>
  <label>
    Message
    <textarea name="message" rows="6" required></textarea>
  </label>
  <button type="submit">Send</button>
</form>
